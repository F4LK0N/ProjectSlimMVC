{
  "require": {
    "php": "8.1.*",
    "slim/slim": "^4.10",
    "slim/psr7": "^1.5",
    "ext-json": "*",
    "ext-xdebug": "3.1.5",
    "ext-mysqli": "*",
    "ext-pdo": "*",
    "ext-pdo_mysql": "*"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/"
